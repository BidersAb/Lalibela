<script type="text/x-handlebars" id="reviews-template">
    {{#each reviews}}
        <div class="review" itemprop="review" itemscope itemtype="http://schema.org/Review">
            {{> rating}}
            <div class="review-content">
                {{> reviewTitle}}
                {{> reviewCategories}}
                {{> ratingImage}}
                {{> photos}}
                {{> reviewText}}
                {{> comments}}
            </div>
        </div>
    {{/each}}
</script>
<script type="text/x-handlebars" id="message-template">
    <div class="message-{{location}}">
        {{#ifEquals messageKey 'hotel.reviews.somethingWentWrong'}}
            Something went wrong, please try again later
        {{/ifEquals}}
        {{#ifEquals messageKey 'hotel.reviews.noMoreReviewsFound'}}
            No more reviews found
        {{/ifEquals}}
    </div>
</script>
<script type="text/x-handlebars" id="reviewTitle-partial">
    {{#if title}}
        <h3 class="title-content" itemprop="headline">{{title}}</h3>
    {{/if}}
</script>
<script type="text/x-handlebars" id="categoryMapping-partial">
    {{#ifEquals categoryId 'Everyone'}}
        Everyone
    {{/ifEquals}}
    {{#ifEquals categoryId 'Couples'}}
        Couples
    {{/ifEquals}}
    {{#ifEquals categoryId 'Families'}}
        Family
    {{/ifEquals}}
    {{#ifEquals categoryId 'GetawayWithFriends'}}
        Friends
    {{/ifEquals}}
    {{#ifEquals categoryId 'BusinessTravelers'}}
        Business
    {{/ifEquals}}
    {{#ifEquals categoryId 'PersonalEvent'}}
        Events
    {{/ifEquals}}
    {{#ifEquals categoryId 'OvernightStayBeforeDestination'}}
        Overnight
    {{/ifEquals}}
    {{#ifEquals categoryId 'Students'}}
        Students
    {{/ifEquals}}
    {{#ifEquals categoryId 'Shopping'}}
        Shopping
    {{/ifEquals}}
    {{#ifEquals categoryId 'Spa'}}
        Spa
    {{/ifEquals}}
    {{#ifEquals categoryId 'Golf'}}
        Golf
    {{/ifEquals}}
    {{#ifEquals categoryId 'Other'}}
        Other
    {{/ifEquals}}
</script>
<script type="text/x-handlebars" id="reviewCategories-partial">
    {{#if reviewerCategories}}
        <div class="review-categories">
            {{#each reviewerCategories}}
                <span itemprop="audience">{{> categoryMapping}}</span>
            {{/each}}
        </div>
    {{/if}}
</script>
<script type="text/x-handlebars" id="ratingImage-partial">
    {{#if ratingImage}}
        <img src="{{ratingImage}}" alt="Trip Advisor rating" class="rating-image" />
    {{/if}}
</script>
<script type="text/x-handlebars" id="rating-partial">
    {{#if ratingOverall}}
        <div class="rating" itemprop="reviewRating" itemscope itemtype="http://schema.org/Rating" data-driven>
            <strong class="rating-number" data-driven itemprop="ratingValue">{{ratingOverall}}</strong>
        </div>
    {{/if}}
</script>
<script type="text/x-handlebars" id="photos-partial">
    {{#if photos}}
        <div class="review-photos">
            {{#each photos}}
                <img src="{{normalUrl}}" alt="{{caption}}" itemprop="image"/>
            {{/each}}
        </div>
    {{/if}}
</script>
<script type="text/x-handlebars" id="reviewText-partial">
    {{#if reviewText}}
        <div class="review-text">
            <p class="review-details" itemprop="reviewBody">{{reviewText}}</p>
            {{#if reviewSubmissionTime}}
                <p class="author">
                    {{#if userNickname}}
                        {{#if userLocation}}
                            By <span itemprop="author">{{userNickname}}</span>, from {{userLocation}} on <span itemprop="dateCreated">{{reviewSubmissionTime}}</span>
                        {{else}}
                            By <span itemprop="author">{{userNickname}}</span> on <span itemprop="dateCreated">{{reviewSubmissionTime}}</span>
                        {{/if}}
                    {{else}}
                        By <span itemprop="author">a verified traveler</span> on <span itemprop="dateCreated">{{reviewSubmissionTime}}</span>
                    {{/if}}
                </p>
            {{/if}}
        </div>
    {{/if}}
</script>
<script type="text/x-handlebars" id="comments-partial">
    {{#if managementResponses}}
        <div class="review-comments">
            {{#each managementResponses}}
                <div class="review-comment" itemprop="comment" itemscope itemtype="http://schema.org/Comment">
                    <span class="title-content">
                        <span class="icon icon-comment" aria-hidden="true"></span><span class="alt">Review comment</span>
                        <span itemprop="author">{{userNickname}}</span> responded on <span itemprop="dateCreated">{{date}}</span>
                    </span>
                    <p>{{response}}</p>
                </div>
            {{/each}}
        </div>
    {{/if}}
</script>
