                var $module = $(".submodule.review-snippets");
                var $btnNext = $module.find("[data-direction=next]");
                var $btnPrev = $module.find("[data-direction=prev]");
                $btnNext.click(function(){
                    manualInteraction = true;
                    changeSelectedReview(1, $module, 5);
                });
                $btnPrev.click(function(){
                    manualInteraction = true;
                    changeSelectedReview(-1, $module, 5);
                });
                //Engagement Tracking
                $btnNext.click(handleTrackOnce("HOT.HIS.RecentReviews.FlipRight"));
                $btnPrev.click(handleTrackOnce("HOT.HIS.RecentReviews.FlipLeft"));
                window.setInterval(function(){
