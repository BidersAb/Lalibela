<div id="map-overlay-background" class="" style="left:0;right:0"></div>
<script type="text/x-handlebars" id="map-overlay-title">
    {{hotelName}}
    <div class="star-rating-wrapper">
        <strong class="star-rating rating-secondary star rating">
            <span class="visuallyhidden">
                {{starRatingTx}} out of 5.0
            </span>
            <span class="icon icon-stars{{starRatingStyle}} stars-grey value-title" aria-hidden="true"></span>
        </strong>
    </div>
    <a class="map-close small-map-close" href="javascript:void(0)" role="button" data-flex-analytics="HOT.HIS.Map.MobileClose">
        <span class="visuallyhidden">Close map</span>
        <span class="icon icon-close" aria-hidden="true"></span>
    </a>
</script>
<script type="text/x-handlebars" id="map-overlay-subTitle">
    <div class="address">
        {{#if address.street}}
            <span class="street-address">{{address.street}}</span>
        {{/if}}
        <span class="city">{{address.city}}</span>
        <span class="province">{{address.province}}</span>
        {{#if ph}}
        <span class="phone-number">
                <span itemprop="telephone">{{ph}}</span>
        </span>
        {{/if}}
    </div>
</script>
<script type="text/x-handlebars" id="map-overlay-legendContent">
    <h3>Points of Interest</h3>
    <ul>
        {{#each poi}}
        <li>
            {{this.name}}
            {{#if this.directDistance.displayValue}}
                - {{this.directDistance.displayValue}} miles
            {{/if}}
        </li>
        {{/each}}
    </ul>
    {{#if highLights}}
    <h3>Location Highlights</h3>
    <ul>
        {{#each highLights}}
        <li>{{this}}</li>
        {{/each}}
    </ul>
    {{/if}}
    {{#if_all region region.name region.description}}
    <h3>{{region.name}}</h3>
    <p>{{region.description}}</p>
    {{/if_all}}
    {{#if_all areaDescriptionTitle areaDescription}}
    <h3>{{areaDescriptionTitle}}</h3>
    <p>{{areaDescription}}</p>
    {{/if_all}}
    {{#if airport}}
    <h3>Nearest Major Airport</h3>
    <p>{{airport}}</p>
    {{/if}}
    {{#if areaInfo}}
    <h3>
        {{#if theme}}
            {{#compare theme "lens"}}
                Attractions
            {{/compare}}
        {{else}}
            Area
        {{/if}}
    </h3>
    <p>{{{areaInfo}}}</p>
    {{/if}}
</script>
<script type="text/x-handlebars" id="hotel-Marker-template">
    <div style="overflow: auto;">
        <h5 class="marker-title">
            <div class="star-rating-wrapper">
                <strong class="star-rating rating-secondary star rating">
                    <span class="visuallyhidden"></span>
                    <span class="icon icon-stars{{starRatingStyle}} stars-grey value-title" aria-hidden="true"></span>
                </strong>
            </div>
        </h5>
        <div class="marker-content secondary">
            {{#if image}}
            <div class="pushpin-photo">
                <figure class="image aspect-ratio1-1" data-media-type="image" data-src="{{image}}" data-aspect-ratio="1-1" data-class="tile-media">
                    <img src="{{image}}" alt="" class="tile-media">
                </figure>
            </div>
            {{/if}}
            <div class="pushpin-address">
                <div class="address">
                    {{#if address.street}}
                        <span class="street-address">{{address.street}}</span>
                    {{/if}}
                    <span class="city">{{address.city}}</span>
                    <span class="province">{{address.province}}</span>
                    {{#if ph}}
                    <span class="phone-number">
                        <span itemprop="telephone">{{ph}}</span>
                    </span>
                    {{/if}}
                </div>
            </div>
        </div>
    </div>
</script>
<script type="text/x-handlebars" id="poi-Marker-template">
    <ul class="poi-pushpin-content">
        {{#if poi.directDistance.displayValue}}
        <li>
            {{poi.directDistance.displayValue}} miles
        </li>
        {{/if}}
        {{#if poi.drivingTime.displayValue}}
        <li>
            <span class="icon icon-cars" aria-hidden="true"></span>
            <span class="alt">Driving:</span>
             {{{poi.drivingTime.displayValue}}} mins
        </li>
        {{/if}}
        {{#if poi.walkingTime.displayValue}}
        <li>
            <span class="icon icon-walk" aria-hidden="true"></span>
            <span class="alt">Walking:</span>
            {{{poi.walkingTime.displayValue}}} mins
        </li>
        {{/if}}
    </ul>
</script>
