        require(['jquery'], function($){
            function changeSelectedReview(direction, $container, reviewCount, canCycle){
                //Find current selected index
                var $selectedReview = $container.find(".review-highlight-item.selected");
                //Find adjacent element
                var selectedIndex = Number($selectedReview.attr('data-index'));
                var adjacentIndex = selectedIndex + direction;
                if(adjacentIndex < 0) { adjacentIndex = canCycle ? (reviewCount - 1) : 0 ; };
                if(adjacentIndex >= reviewCount){ adjacentIndex = canCycle ? 0 : (reviewCount - 1); }
                //If no change, do nothing.
                if(adjacentIndex == selectedIndex) { return; }
                $newSelectedReview = $container.find(".review-highlight-item[data-index=" + adjacentIndex + "]");
                //Remove selection status
                $selectedReview.removeClass("selected");
                //Apply Selection Status
                $newSelectedReview.addClass("selected");
            }
