                        require('flexAnalytics', function(FlexAnalytics) {
                            var flexAnalytics = new FlexAnalytics();
                            flexAnalytics.trackAction(rfrr);
                        });
                        //This will only be executed if Flex_UIS_Prime_Migration_Enabled is true
                        require('flexAnalyticsPrime', function(FlexAnalyticsPrime) {
                            FlexAnalyticsPrime().trackAction(rfrr);
                        });
