    require(["jquery", "throttle", "findOnscreen", "uitk"], function($, throttle, findOnscreen, uitk){
        var SEEK = "middle";
        var SELECTOR =  ".hotel-section-links a.hotel-section-links__item";
        var BROWSER  =   navigator.userAgent.match(/msie/i);
        var USERAGENT =  navigator.userAgent;
        var REGEXP  =  new RegExp("Trident/.*rv:([0-9]{1,}[\.0-9]{0,})");
        var getItems = function(){
            return $(SELECTOR).map(function(index, item){
                var pieces = (item.href || "").split('#');
                var selector = pieces.length >= 2 ? ("#" + pieces[1]) : 'body';
                var offset = $(selector).offset();
                return {
                    link: $(item),
                    item: $(selector)[0],
                    top: offset && offset.top
                };
            });
        }
        if (REGEXP.exec(USERAGENT) == null)  {
            if(!BROWSER){
                findOnscreen.onScroll(getItems, function(activeSection){
                $('.hotel-section-links a').removeClass('active');
                    if(activeSection){
                         activeSection.link.addClass('active');
                    }
                }, SEEK, false);
            }
        }
        $(SELECTOR).on(uitk.clickEvent, function(e) {
            $('.hotel-section-links a').removeClass('active');
            $(this).addClass('active');
            e.preventDefault();
            var section = $(e.currentTarget.attributes.href.value);
            $('html, body').stop().animate({
                scrollTop: section.offset().top - 140
            }, 500, 'swing');
        })
    });
